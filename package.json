{
  "name": "anyhow-fitness",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "kill-ports": "powershell -ExecutionPolicy Bypass -File kill-ports.ps1",
    "dev:backend": "ts-node-dev --respawn --transpile-only -P src/tsconfig.json src/index.ts",
    "dev:frontend": "vite --port 5173 --open",
    "frontend": "vite --port 5173",
    "dev:clean": "npm run kill-ports && concurrently \"npm run dev:backend\" \"npm run frontend\"",
    "dev": "concurrently \"npm run dev:backend\" \"npm run frontend\"",
    "build": "vite build",
    "build:frontend": "vite build",
    "build:backend": "tsc -p src/tsconfig.json",
    "start": "node src/dist/src/index.js",
    "migrate": "ts-node -P tsconfig.node.json migrations/runMigrations.ts",
    "migrate:production": "cross-env NODE_ENV=production ts-node -P tsconfig.node.json migrations/runMigrations.ts",
    "migrate:test": "ts-node -P tsconfig.node.json src/utils/testDb.ts",
    "preview": "vite preview",
    "pm2:start": "cross-env NODE_ENV=production pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop anyhow-fitness-backend",
    "pm2:restart": "pm2 restart anyhow-fitness-backend",
    "start:app": "pm2 start ecosystem.config.js && start http://localhost:5173 && npm run frontend",
    "pm2:delete": "pm2 delete anyhow-fitness-backend",
    "pm2:logs": "pm2 logs anyhow-fitness-backend",
    "test:api": "cross-env NODE_ENV=test ts-node-dev src/index.ts",
    "test:db": "cross-env NODE_ENV=test ts-node src/utils/testDb.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "recharts": "^2.8.0",
    "twilio": "^4.18.0",
    "undici": "^5.28.4",
    "winston": "^3.17.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.6.2",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.49.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.29",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vite": "^4.4.9"
  }
}
